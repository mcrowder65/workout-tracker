{
  "name": "workout-tracker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@firebase/app": "0.3.8",
    "@firebase/auth": "0.9.2",
    "@firebase/database": "0.3.12",
    "@material-ui/core": "3.9.1",
    "@material-ui/icons": "3.0.2",
    "@material-ui/lab": "3.0.0-alpha.30",
    "lodash.compose": "2.4.1",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "react-scripts": "2.1.3",
    "shortid": "2.2.14",
    "use-local-storage-set-state": "1.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "linter": "eslint src --ext .js",
    "test": "npm run linter && react-scripts test",
    "eject": "react-scripts eject",
    "bundlesize": "bundlesize",
    "analyze": "source-map-explorer build/static/js/main.*",
    "analyze-npm": "source-map-explorer build/static/js/1.*",
    "prettier": "prettier --write \"src/**/*.js\""
  },
  "bundlesize": [
    {
      "path": "build/static/js/*.js",
      "maxSize": "90 kB"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier"
    }
  },
  "eslintConfig": {
    "extends": [
      "mcrowder65"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "bundlesize": "0.17.1",
    "eslint-config-mcrowder65": "0.0.36",
    "husky": "1.3.1",
    "jest-dom": "3.0.1",
    "react-testing-library": "5.4.4",
    "source-map-explorer": "1.6.0"
  }
}
